<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="58dp"
        android:layout_height="72dp"
        android:background="@drawable/check"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="58dp"
        android:layout_height="57dp"
        android:background="@drawable/cancel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/BookimageButton"
        android:layout_width="102dp"
        android:layout_height="163dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/book"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/BookimageButton3"
        android:layout_width="104dp"
        android:layout_height="80dp"
        android:background="@drawable/book"
        app:layout_constraintStart_toEndOf="@+id/BookimageButton"
        app:layout_constraintTop_toBottomOf="@+id/BookimageButton2" />

    <ImageButton
        android:id="@+id/BookimageButton2"
        android:layout_width="103dp"
        android:layout_height="83dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/book"
        app:layout_constraintStart_toEndOf="@+id/BookimageButton"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/BookimageButton4"
        android:layout_width="65dp"
        android:layout_height="52dp"
        android:layout_marginTop="95dp"
        android:background="@drawable/book"
        app:layout_constraintStart_toEndOf="@+id/imageButton11"
        app:layout_constraintTop_toBottomOf="@+id/input_ExplanationeditTextTextPersonName" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="36dp"
        android:text="リードリストを公開"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_ExplanationeditTextTextPersonName" />

    <EditText
        android:id="@+id/input_nameeditTextTextPersonName"
        android:layout_width="245dp"
        android:layout_height="42dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="280dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/input_ExplanationeditTextTextPersonName"
        android:layout_width="253dp"
        android:layout_height="47dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_nameeditTextTextPersonName" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="16dp"
        android:text="リードリストの作成、編集"
        app:layout_constraintStart_toEndOf="@+id/imageButton5"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="33dp"
        android:layout_height="27dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/cancelbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_ExplanationeditTextTextPersonName" />

    <TextView
        android:id="@+id/Book_nametextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginTop="106dp"
        app:layout_constraintStart_toEndOf="@+id/BookimageButton4"
        app:layout_constraintTop_toBottomOf="@+id/input_ExplanationeditTextTextPersonName" />

    <TextView
        android:id="@+id/Author_nametextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginTop="140dp"
        app:layout_constraintStart_toEndOf="@+id/BookimageButton4"
        app:layout_constraintTop_toBottomOf="@+id/input_ExplanationeditTextTextPersonName" />

</androidx.constraintlayout.widget.ConstraintLayout>