<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/sort"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="並び替え："
        app:layout_constraintEnd_toStartOf="@+id/spinner"
        app:layout_constraintTop_toTopOf="@+id/spinner" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="113dp"
        android:layout_height="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:entries="@array/planets_array"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/book_Image1"
        android:layout_width="72dp"
        android:layout_height="94dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image2"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image1"
        app:layout_constraintTop_toTopOf="@+id/book_Image1"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image3"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image2"
        app:layout_constraintTop_toTopOf="@+id/book_Image2"
        app:srcCompat="@drawable/book" />

    <Button
        android:id="@+id/list_Button1"
        android:layout_width="237dp"
        android:layout_height="112dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/readrist_ranking_frame"
        android:gravity="bottom|right"
        android:paddingStart="40dp"
        android:paddingTop="50dp"
        android:paddingEnd="45dp"
        android:paddingBottom="10dp"
        android:text="一覧を見る"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sort" />

    <ImageView
        android:id="@+id/book_Image4"
        android:layout_width="72dp"
        android:layout_height="94dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/book_Image1"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image5"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image4"
        app:layout_constraintTop_toTopOf="@+id/book_Image4"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image6"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image5"
        app:layout_constraintTop_toTopOf="@+id/book_Image5"
        app:srcCompat="@drawable/book" />

    <Button
        android:id="@+id/list_Button2"
        android:layout_width="237dp"
        android:layout_height="112dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/readrist_ranking_frame"
        android:gravity="bottom|right"
        android:paddingStart="40dp"
        android:paddingTop="50dp"
        android:paddingEnd="45dp"
        android:paddingBottom="10dp"
        android:text="一覧を見る"
        app:layout_constraintStart_toStartOf="@+id/list_Button1"
        app:layout_constraintTop_toBottomOf="@+id/list_Button1" />

    <ImageView
        android:id="@+id/book_Image7"
        android:layout_width="72dp"
        android:layout_height="94dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/book_Image4"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image8"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image7"
        app:layout_constraintTop_toTopOf="@+id/book_Image7"
        app:srcCompat="@drawable/book" />

    <ImageView
        android:id="@+id/book_Image9"
        android:layout_width="56dp"
        android:layout_height="62dp"
        app:layout_constraintStart_toEndOf="@+id/book_Image8"
        app:layout_constraintTop_toTopOf="@+id/book_Image8"
        app:srcCompat="@drawable/book" />

    <Button
        android:id="@+id/list_Button3"
        android:layout_width="237dp"
        android:layout_height="112dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/readrist_ranking_frame"
        android:gravity="bottom|right"
        android:paddingStart="40dp"
        android:paddingTop="50dp"
        android:paddingEnd="45dp"
        android:paddingBottom="10dp"
        android:text="一覧を見る"
        app:layout_constraintStart_toStartOf="@+id/list_Button2"
        app:layout_constraintTop_toBottomOf="@+id/list_Button2" />

    <ImageButton
        android:id="@+id/back_Butoon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/back"
        app:layout_constraintEnd_toStartOf="@+id/page_Button1"
        app:layout_constraintTop_toTopOf="@+id/page_Button1" />

    <Button
        android:id="@+id/page_Button1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="32dp"
        android:background="#00000000"
        android:text="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/book_Image7" />

    <Button
        android:id="@+id/page_Button2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="#00000000"
        android:text="2"
        app:layout_constraintStart_toEndOf="@+id/page_Button1"
        app:layout_constraintTop_toTopOf="@+id/page_Button1" />

    <Button
        android:id="@+id/page_Button3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="#00000000"
        android:text="3"
        app:layout_constraintStart_toEndOf="@+id/page_Button2"
        app:layout_constraintTop_toTopOf="@+id/page_Button2" />

    <ImageButton
        android:id="@+id/move_Button"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/move"
        app:layout_constraintStart_toEndOf="@+id/page_Button3"
        app:layout_constraintTop_toTopOf="@+id/page_Button3" />

</androidx.constraintlayout.widget.ConstraintLayout>